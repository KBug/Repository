<addons>
<addon id="plugin.parrot.tv"
       name="Parrot TV"
       version="9.0.0"
       provider-name="Parrot Developers">
  <requires>
    <import addon="xbmc.python" version="2.2.0"/>
    <import addon="script.module.requests"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Parrot TV is free project by Parrot Developers</summary>
    <language>en</language>
    <description lang="en">Free CZ/SK TV for all. These links are freely avalibe on WWW.</description>
    <description lang="cz">CZ / SK TV Zdarma pro všechny.</description>
    <description lang="sk">CZ/SK Televízia zadarmo pre všetkých.</description>
     <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.parrot.tv+matrix"
version="2.0.0"
name="Parrot TV"
provider-name="Parrot Developers + HereIronman7746">
<requires>
  <import addon="xbmc.python" version="3.0.0"/>
</requires>
<extension point="xbmc.python.pluginsource" library="main.py">
  <provides>video</provides>
</extension>
<extension point="xbmc.addon.metadata">
  <summary lang="en">Free tv channels. If you want movies download addon "sosac.ph" From kodi XBMC cz</summary>
  <description lang="en_GB">Free tv channels. If you want movies download addon "sosac.ph" From kodi XBMC cz</description>
  <disclaimer lang="en_GB">All links are freely avalibe on www.</disclaimer>
  <assets>
